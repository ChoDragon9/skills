/* Layout start
------------------------------------------------*/

#wrapper { width:100%; float:left; background:url(/img/menubg.png) repeat-x; }
	#wrap { width:900px; margin:0 auto; }
		#header { width:900px; height:120px; float:left; }
			#navi { width:900px; height:25px; float:left; }
				#navi > li { float:right; margin-left:10px; font-size:11px; margin-top:7px; color:#fff; }
				#navi > li > a { color:#fff; }
			#logo { width:200px; height:78px; float:left; padding-top:17px; }
			#menu { width:700px; height:95px; float:left; }
				#menu > ul { margin-top:28px; }
				#menu > ul > li:first-child { margin-left:0; }
				#menu > ul > li { float:left; margin-left:29px; }
				#menu > ul > li > ul { position:absolute; display:none; }
				html[xmlns] #menu > ul > li > ul { position:absolute; margin-top:31px; display:none; }
				*:first-child+html #menu > ul > li > ul { position:absolute; margin-top:30px; display:none; }
				#menu > ul > li > ul > li { font-size:12px; float:left; font-weight:bold; margin-right:15px; }
				#menu > ul > li > ul > li > a { color:#fff; }
#footer_out { width:100%; height:70px; float:left; background:#e4e4e4; text-align:right; }
	#footer { width:900px; height:50px; margin:0 auto; font-size:11px; text-transform:uppercase; padding-top:20px; color:#333333; }
		#footer > li { line-height:18px; }

/* Layout end
------------------------------------------------*/