/* Common start
------------------------------------------------*/

* { margin:0; padding:0; font-family:dotum; }
img { vertical-align:top; border:none; }
ul { list-style:none; }
a { text-decoration:none; color:#666; }
a:hover { text-decoration:underline; }

input[type="reset"].bt1,input[type="submit"].bt1,input[type="button"].bt1 { border:none; background:url(/img/btbg.png) no-repeat; width:70px; height:30px; font-size:11px; letter-spacing:-1px; padding-top:1px; cursor:pointer; }
input[type="reset"].bt2,input[type="submit"].bt2,input[type="button"].bt2 { border:none; background:url(/img/btbg2.png) no-repeat; width:70px; height:30px; font-size:11px; letter-spacing:-1px; color:#fff; padding-top:1px; cursor:pointer; }
input[type="text"],input[type="password"]{ border:solid 1px #dbdbdb; border-top:solid 1px #aaa; border-left:solid 1px #aaa; height:22px; }
textarea { border:solid 1px #aaa;  width:490px; height:175px;}
input[type="file"] { border:solid 1px #dbdbdb; height:22px; }

/* Common end
------------------------------------------------*/

